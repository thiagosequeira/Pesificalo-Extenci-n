{
  "manifest_version": 3,
  "name": "Pesificalo: Dolares a Pesos Argentinos",
  "version": "0.0.1",
  "description": "Convertí los precios de USD a ARS, calculá impuestos rápido y sin tener que hacer tanto quilombo.",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
        "16": "/images/icon16.png",
        "32": "/images/icon32.png",
        "48": "/images/icon48.png",
        "128": "/images/icon128.png"
    }
  },
  "icons": {
    "16": "/images/icon16.png",
    "32": "/images/icon32.png",
    "48": "/images/icon48.png",
    "128": "/images/icon128.png"
  }
}